{
    "name": "nucarf/php-elastic-query",
    "description": "PHP ElasticSearch query，接口模仿 Laravel Eloquent.",
    "type": "library",
    "version": "1.2",
    "authors": [
        {
            "name": "lihongwei",
            "email": "lihongwei@nucarf.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "elasticsearch/elasticsearch": "^6.0",
        "ongr/elasticsearch-dsl": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Nucarf\\Elastic\\": "src/Elastic"
        }
    },
    "require-dev": {
    },
    "autoload-dev": {
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http": false
    }
}
